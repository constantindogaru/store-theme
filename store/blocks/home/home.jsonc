    "props": {
      "horizontalAlign": "center"
    }
  },

  "flex-layout.row#recommended-categories-right-row-2-col-1-image": {
    "children": ["image#recommended-category-highlight-4"],
    "props": {
      "blockClass": "imageOverflow"
    }
