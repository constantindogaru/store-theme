      "flex-layout.row#deals",
      "rich-text#shelf-title",
      // "flex-layout.row#shelf",
      "info-card#home",
      "rich-text#question",
      "rich-text#link"
    ]
  },

  "list-context.image-list#demo": {
    "children": ["slider-layout#demo-images"],
    "props": {
      "height": 720,
      "images": [
        {
          "image": "https://www.fine-food.ro/wp-content/uploads/2020/03/fine-food-slider-13.jpg",
          "mobileImage": "https://www.fine-food.ro/wp-content/uploads/2020/03/fine-food-slider-13.jpg"
        },
        {
          "image": "https://www.fine-food.ro/wp-content/uploads/2020/03/fine-food-slider-19.jpg",
          "mobileImage": "https://www.fine-food.ro/wp-content/uploads/2020/03/fine-food-slider-19.jpg"
        },
        {
          "image": "https://www.fine-food.ro/wp-content/uploads/2020/03/fine-food-slider-3.jpg",
          "mobileImage": "https://www.fine-food.ro/wp-content/uploads/2020/03/fine-food-slider-3.jpg"
        },
        {
          "image": "https://www.fine-food.ro/wp-content/uploads/2020/05/Slider-fructe-de-mare.jpg",
          "mobileImage": "https://www.fine-food.ro/wp-content/uploads/2020/05/Slider-fructe-de-mare.jpg"
        }
      ]
    }
  },
  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },

  "flex-layout.row#recommended-categories-wrapper": {
    "children": ["flex-layout.col#recommended-categories-wrapper"],
    "props": {
      "marginTop": 10,
      "marginBottom": 10,
      "horizontalAlign": "center",
      "colGap": 0
    }
  },

  "flex-layout.col#recommended-categories-wrapper": {
    "children": [
      "rich-text#recommended-categories-title",
      "flex-layout.row#recommended-categories-row"
    ],
    "props": {
      "horizontalAlign": "center",
      "colGap": 7
    }
  },

  "rich-text#recommended-categories-title": {
    "props": {
      "text": "COLECȚIILE FINE FOOD\n### CATEGORII RECOMADATE\nProdusele noastre premium",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },

  "flex-layout.row#recommended-categories-row": {
    "children": [
      "flex-layout.col#recommended-category-highlight-1",
      "flex-layout.col#recommended-categories-highlights-2"
    ],
    "props": {
      "marginTop": 5
    }
  },

  "flex-layout.col#recommended-category-highlight-1": {
    "children": [
      "flex-layout.row#recommended-category-highlight-1-image",

      "rich-text#recommended-category-highlight-1"
    ],
    "props": {
      "horizontalAlign": "center"
    }
  },

  "flex-layout.row#recommended-category-highlight-1-image": {
    "children": ["image#recommended-category-highlight-1"],

    "props": {
      "blockClass": "imageOverflow"
    }
  },

  "image#recommended-category-highlight-1": {
    "props": {
      "src": "https://www.fine-food.ro/wp-content/uploads/2020/03/cat-kids-600x600.jpg",
      "maxHeight": 600,
      "maxWidth": 600,
      "blockClass": "zoomIn"
    }
  },

  "rich-text#recommended-category-highlight-1": {
    "props": {
      "text": "### PRODUSE PENTRU COPII"
    }
  },

  "flex-layout.col#recommended-categories-highlights-2": {
    "children": [
      "flex-layout.row#recommended-categories-right-row-1",
      "flex-layout.row#recommended-categories-right-row-2"
    ]
  },

  "flex-layout.row#recommended-categories-right-row-1": {
    "children": [
      "flex-layout.col#recommended-categories-right-row-1-col-1",
      "flex-layout.col#recommended-categories-right-row-1-col-2"
    ],
    "props": {
      "horizontalAlign": "left"
    }
  },

  "flex-layout.row#recommended-categories-right-row-2": {
    "children": [
      "flex-layout.col#recommended-categories-right-row-2-col-1",
      "flex-layout.col#recommended-categories-right-row-2-col-2"
    ]
  },

  "flex-layout.col#recommended-categories-right-row-1-col-1": {
    "children": [
      "flex-layout.row#recommended-categories-right-row-1-col-1-image",
      "rich-text#recommended-category-highlight-2"
